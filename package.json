{
  "name": "general-backend",
  "version": "1.1.0",
  "description": "This is a general backend server built with Express and TypeScript, featuring user authentication, session management, and database interactions using Prisma ORM with PostgreSQL.",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "prebuild": "prisma generate",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "postgresql",
    "authentication",
    "session-management",
    "backend"
  ],
  "author": "Masood Acheampong",
  "license": "ISC",
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "pg": "^8.18.0",
    "prisma": "^7.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/node": "^25.2.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "ts-node": {
    "files": true
  }
}
